<%- include('header.ejs'); %>
<%- include('navbar.ejs'); %>
    <div class="container">
      <div class="error-title col-sm-offset-1 col-sm-11">
        <h1><%= model.error %></h1>
      </div>
      <div class="error-description col-sm-offset-1 col-sm-11">
        <p><%= model.message %></p>
      </div>
      <% if (model.next) { -%>
      <div class="error-button col-sm-offset-1 col-sm-11">
        <a href="<%= model.next.href %>"><%= model.next.description %></a>
      </div>
      <% } -%>
    </div>
<%- include('footer.ejs'); %>